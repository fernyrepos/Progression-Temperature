<?xml version="1.0" encoding="utf-8"?>
<Patch>

                <Operation Class="PatchOperationFindMod">
                    <mods>
                    <li>Replace Stuff - Continued</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                    <operations>
                    <li Class="PatchOperationAdd">
                        <xpath>Defs</xpath>
                        <value>
                            <VEF.Buildings.HiddenDesignatorsDef>
                                <defName>FernyVETemperatureReplaceStuffProgTemperature_HiddenDesignators</defName>
                                <hiddenDesignators>			
                                    <li>Cooler_Over</li>
                                    <li>Vent_Over</li>
                                </hiddenDesignators>							
                            </VEF.Buildings.HiddenDesignatorsDef>
                        </value>
                    </li>
                    </operations>
                    </match>
                </Operation>


	<!--  Relabel shit     -->
                <Operation Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="Heater"]/label</xpath>
                    <value>
                        <label>standing heater</label>
                    </value>
                </Operation>
                <Operation Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="VTE_WallHeater"]/label</xpath>
                    <value>
                        <label>heater</label>
                    </value>
                </Operation>

</Patch>