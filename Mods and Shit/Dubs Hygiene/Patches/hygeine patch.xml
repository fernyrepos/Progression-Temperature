<?xml version="1.0" encoding="utf-8" ?>          
<Patch>       


                <Operation Class="PatchOperationFindMod">
                    <mods>
                    <li>Vanilla Temperature Expanded</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                    <operations>
                    <li Class="PatchOperationRemove">
                    <xpath>Defs/ResearchProjectDef[defName="MultiSplitAirCon"]</xpath>
                    </li>
                    <li Class="PatchOperationAdd">
                        <xpath>Defs</xpath>
                        <value>
                            <VEF.Buildings.HiddenDesignatorsDef>
                                <defName>FernyDubsHygieneVETemperatureProgTemperature_HiddenDesignators</defName>
                                <hiddenDesignators>			
                                    <li>AirConOutdoorUnit</li>
                                    <li>AirconIndoorUnit</li>
                                    <li>FreezerUnit</li>
                                    <li>airPipe</li>
                                    <li>VTE_SmallCeilingFan</li>
                                    <li>VTE_LargeCeilingFan</li>
                                    <li>airPipeHidden</li>
                                </hiddenDesignators>							
                            </VEF.Buildings.HiddenDesignatorsDef>
                        </value>
                    </li>
                    </operations>
                    </match>
                </Operation>

                <Operation Class="PatchOperationReplace">
                    <xpath>Defs/ResearchProjectDef[defName="LogBoilers"]/label</xpath>
                    <value>
                        <label>Water-based heating</label>
                    </value>
                </Operation>
                <Operation Class="PatchOperationReplace">
                    <xpath>Defs/ResearchProjectDef[defName="CentralHeating"]/label</xpath>
                    <value>
                        <label>Fluid heat exchange</label>
                    </value>
                </Operation>
                <Operation Class="PatchOperationReplace">
                    <xpath>Defs/ResearchProjectDef[defName="GeothermalHeating"]/label</xpath>
                    <value>
                        <label>Geothermal water heater</label>
                    </value>
                </Operation>
                <Operation Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="SolarHeater"]/label</xpath>
                    <value>
                        <label>Solar water heater</label>
                    </value>
                </Operation>
                <Operation Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="GeothermHeater"]/label</xpath>
                    <value>
                        <label>Geothermal water heater</label>
                    </value>
                </Operation>

</Patch>